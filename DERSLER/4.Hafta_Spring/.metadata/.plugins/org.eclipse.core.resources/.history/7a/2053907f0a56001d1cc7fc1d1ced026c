package kodlama.io.rentACar.entities.concretes;

public class Brand {
	private inte id;
	private String name;
	
	public Brand() {
		super();
	}

	public Brand(inte id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public inte getId() {
		return id;
	}

	public void setId(inte id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
	
	
}
